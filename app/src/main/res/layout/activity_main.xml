<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <Button
            android:id="@+id/no_change_thread_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="无线程切换"/>
        <Button
            android:id="@+id/change_thread_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="带线程切换"/>
        <Button
                android:id="@+id/rx_map"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:onClick="rxClickMap"
                android:text="rx_map"/>
        <Button
                android:id="@+id/rx_filter"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:onClick="rxClickFilter"
                android:text="rx_filter"/>
        <Button
                android:id="@+id/rx_flat_map"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:onClick="rxClickFlatMap"
                android:text="rx_flat_map"/>
        <Button
                android:id="@+id/rx_zip"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:onClick="rxClickZip"
                android:text="rx_zip"/>
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>